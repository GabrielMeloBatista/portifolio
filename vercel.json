{
    "version": 2,
    "framework": null,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": { "distDir": "public/build" }
        },
        {
            "src": "public/index.php",
            "use": "vercel-php@0.6.2"
        }
    ],
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/storage/(.*)",
            "dest": "/public/storage/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ],
    "env": {
        "APP_NAME": "Laravel",
        "APP_ENV": "production",
        "APP_KEY": "base64:NaR+wRXN0PB8rAC77d7FMQ+Ub3AoDLNBnroPfDP6u1I=",
        "APP_DEBUG": "false",
        "APP_URL": "https://portifolio-coral-alpha.vercel.app/",

        "LOG_CHANNEL": "stderr",
        "DB_CONNECTION": "sqlite",
        "DB_DATABASE": "/tmp/database.sqlite",

        "CACHE_DRIVER": "array",
        "FILESYSTEM_DISK": "local",
        "QUEUE_CONNECTION": "sync",
        "SESSION_DRIVER": "cookie",
        "SESSION_LIFETIME": "120",

        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp/views",
        "CACHE_STORES_file_PATH": "/tmp/cache/data",
        "LOG_STDERR_FORMATTER": "Monolog\\Formatter\\JsonFormatter"
    },
    "outputDirectory": "public"
}